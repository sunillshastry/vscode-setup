{
  "workbench.iconTheme": "material-icon-theme",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.fontFamily": "JetBrains Mono, SF Mono, Menlo",
  "editor.wordWrap": "on",
  "files.exclude": {
    "**/.DS_Store": false,
    "**/.git": false
  },
  "editor.lineHeight": 30,
  "editor.cursorWidth": 2,
  "editor.fontWeight": 400,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "workbench.editor.closeOnFileDelete": true,
  "cSpell.userWords": ["anjana", "harsha", "shastri", "shastry", "sunil"],
  "editor.useTabStops": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 14,
  "template-string-converter.enable": true,
  "template-string-converter.addBracketsToProps": true,
  "template-string-converter.autoRemoveTemplateString": true,
  "workbench.colorCustomizations": {
    "[Material Theme Ocean High Contrast]": {
      "activityBarBadge.background": "#28FFBF",
      "activityBar.activeBorder": "#28FFBF",
      "list.activeSelectionForeground": "#28FFBF",
      "list.inactiveSelectionForeground": "#28FFBF",
      "list.highlightForeground": "#28FFBF",
      "scrollbarSlider.activeBackground": "#28FFBF50",
      "editorSuggestWidget.highlightForeground": "#28FFBF",
      "textLink.foreground": "#28FFBF",
      "progressBar.background": "#28FFBF",
      "pickerGroup.foreground": "#28FFBF",
      "tab.activeBorder": "#28FFBF",
      "notificationLink.foreground": "#28FFBF",
      "editorWidget.resizeBorder": "#28FFBF",
      "editorWidget.border": "#28FFBF",
      "settings.modifiedItemIndicator": "#28FFBF",
      "settings.headerForeground": "#28FFBF",
      "panelTitle.activeBorder": "#28FFBF",
      "breadcrumb.activeSelectionForeground": "#28FFBF",
      "menu.selectionForeground": "#28FFBF",
      "menubar.selectionForeground": "#28FFBF",
      "editor.findMatchBorder": "#28FFBF",
      "selection.background": "#28FFBF40",
      "statusBarItem.remoteBackground": "#28FFBF"
    }
  },
  "materialTheme.accent": "Purple",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.fontSize": 14,
  "editor.cursorBlinking": "expand",
  "importCost.smallPackageSize": 50,
  "importCost.mediumPackageSize": 100,
  "importCost.smallPackageColor": "#80ED99",
  "importCost.mediumPackageColor": "#FEE440",
  "importCost.largePackageColor": "#FF8243",
  "importCost.typescriptExtensions": ["\\.tsx?$"],
  "importCost.javascriptExtensions": ["\\.jsx?$"],
  "importCost.bundleSizeDecoration": "both",
  "importCost.showCalculatingDecoration": true,
  "importCost.debug": false,
  "workbench.colorTheme": "Material Theme Ocean High Contrast",
  "editor.parameterHints.cycle": true,
  "javascript.suggest.completeFunctionCalls": true,
  "typescript.suggest.completeFunctionCalls": true,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "bracket-pair-colorizer-2.colors": [
    "#99FEFF",
    "#FF5959",
    "#49FF00",
    "#009DAE",
    "#F2F013"
  ],
  "editor.insertSpaces": false,
  "editor.detectIndentation": false,
  "C_Cpp.updateChannel": "Insiders",
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript", "typescript", "react", "vue", "html", "css"]
}
